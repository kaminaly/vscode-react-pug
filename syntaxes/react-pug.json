{
	"fileTypes": ["js", "jsx", "ts", "tsx"],
	"injectionSelector": "L:source -comment -string",
	"patterns": [
		{
			"include": "#pug-tagged-template"
		}
	],
	"repository": {
		"pug-tagged-template": {
			"begin": "pug`",
			"end": "`",
			"patterns": [
				{
					"include": "text.pug"
				}
			]
		}
	},
	"scopeName": "react.pug"
}
